<h1>We found these vans for you:</h1>

<div>
  <% @vans.each do |van| %>
    <p><%= van.title %></p>
    <p><%= van.description %></p>
    <p><%= van.location %></p>
    <p><%= van.brand %></p>
    <p><%= van.category %></p>
    <p>Seats: <%= van.seats %></p>
    <p>Owner: <%= van.user.first_name %></p>
  <% end %>
</div>


